                    "MaxSimultaneousConnections": -1,
                    "HostReadBuffersPerSecond": -1,
                    "HostWriteBuffersPerSecond": -1,
                    "ClientReadBuffersPerSecond": -1,
                    "ClientWriteBuffersPerSecond": -1
                }
            }
        },
        "anonymous": {
            "Password": "anonymous",
            "Restrictions": {
                "Blacklist": [],
                "Whitelist": [],
                "Rate": {
                    "MaxSimultaneousConnections": 100,
                    "HostReadBuffersPerSecond": 2080,
                    "HostWriteBuffersPerSecond": 2080,
                    "ClientReadBuffersPerSecond": 2080,
                    "ClientWriteBuffersPerSecond": 2080
                }
            }
        }
    }
